    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Migra",
            "type": "python",
            "request": "launch",
            "program": "/workspace/pydbmigra.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "postgres://root@dbmigra/enforce",
                "postgres://root@dbmigra/Salesforce_staging",
                "--unsafe",
                "--tables_only",
                "--schema=enforce,cfpb3",
                "--no_drops"
            ]
        },
        {
            "name": "Migrabd",
            "type": "python",
            "request": "launch",
            "program": "/workspace/pydbmigra.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "postgres://root@dbmigra/Salesforce_staging",
                "postgres://root@dbmigra/enforce",
                "--unsafe",
                "--tables_only",
                "--schema=cfpb3,enforce"
            ]
        },
        {
            "name": "Migra-Diff-schema",
            "type": "python",
            "request": "launch",
            "program": "/workspace/pydbmigra.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "postgres://root@dbmigra/postgres",
                "postgres://root@dbmigra/root",
                "--schema=staging,production",
                "--unsafe",
                "--tables=staging,production",
                "--tables_only"
            ]
        }
    ]
}